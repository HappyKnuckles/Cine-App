{
	"appId": "io.ionic.starter",
	"appName": "Cine App",
	"webDir": "www",
	"bundledWebRuntime": false,
	
	"android": {
		"server": {
			"allowList": ["https://cors-anywhere.herokuapp.com", "https://www.cinecitta.de"]
		},
		"allowNavigation": [],
		"allowedNavigations": [
		  "https://cors-anywhere.herokuapp.com",
		  "https://cinecitta.de/*"
		],
		"allowMixedContent": true,
		"permissions": ["INTERNET"],
		"allowedIntents": [
		  "geo:*",
		  "mailto:*",
		  "sms:*",
		  "tel:*",
		  "http://*/*" ,
    	  "https://*/*" 
		],
		"plugins": {
			"InAppBrowser": {
			  "appId": "io.ionic.starter"
			}
	  }
	}  
}
